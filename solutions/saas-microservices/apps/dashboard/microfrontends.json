{
    "$schema": "./node_modules/@vercel/microfrontends/schema/schema.json",
    "applications": {
        "saas-microservices-dashboard": {
            "development": {
                "fallback": "dashboard-acme.vercel.app"
            }
        },
        "saas-microservices-api-dashboard": {
            "routing": [
                {
                    "paths": [
                        "/api/dashboard/:path*"
                    ]
                }
            ]
        },
        "saas-microservices-api-users": {
            "routing": [
                {
                    "paths": [
                        "/api/users/:path*"
                    ]
                }
            ]
        }
    }
}